<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme" parent="@miui:style/V5.Theme.Light">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">stateAlwaysHidden|adjustPan</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/fade_in</item>
        <item name="android:windowExitAnimation">@anim/fade_out</item>
    </style>
    <style name="FolderSettingButton">
        <item name="android:textSize">20.0sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="FolderIconItem">
        <item name="android:paddingLeft">1.0dip</item>
        <item name="android:paddingTop">1.0dip</item>
        <item name="android:paddingRight">1.0dip</item>
        <item name="android:paddingBottom">1.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="WorkspaceIconTitle">
        <item name="android:textSize">@dimen/workspace_icon_text_size</item>
        <item name="android:textColor">@color/icon_title_text</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/icon_title_padding_side</item>
        <item name="android:paddingRight">@dimen/icon_title_padding_side</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">@color/icon_title_text_shadow</item>
        <item name="android:shadowDx">@dimen/workspace_icon_text_shadow_dx</item>
        <item name="android:shadowDy">@dimen/workspace_icon_text_shadow_dy</item>
        <item name="android:shadowRadius">@dimen/workspace_icon_text_shadow_radius</item>
    </style>
    <style name="WorkspaceIconTitle.dark" parent="@style/WorkspaceIconTitle">
        <item name="android:textColor">@color/icon_title_text_dark</item>
        <item name="android:shadowColor">@color/icon_title_text_shadow_light</item>
    </style>
    <style name="WorkspaceIconTitle.Thumbnail" parent="@style/WorkspaceIconTitle">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">@dimen/thumbnail_title_width</item>
        <item name="android:layout_height">25.0dip</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:layout_marginRight">5.0dip</item>
        <item name="android:drawablePadding">5.0dip</item>
    </style>
    <style name="WorkspaceIconTitle.Thumbnail.dark" parent="@style/WorkspaceIconTitle.Thumbnail">
        <item name="android:textColor">@color/icon_title_text_dark</item>
        <item name="android:shadowColor">@color/icon_title_text_shadow_light</item>
    </style>
    <style name="WorkspaceIconTitle.folder" parent="@style/WorkspaceIconTitle">
        <item name="android:textColor">@color/folder_icon_title_text</item>
        <item name="android:shadowColor">@color/folder_icon_title_text_shadow</item>
    </style>
    <style name="WorkspaceIconTitle.folder.dark" parent="@style/WorkspaceIconTitle.folder">
        <item name="android:textColor">@color/folder_icon_title_text_dark</item>
        <item name="android:shadowColor">@color/folder_icon_title_text_shadow_light</item>
    </style>
    <style name="WorkspaceIconTitle.notification" parent="@style/WorkspaceIconTitle">
        <item name="android:textSize">@dimen/workspace_icon_text_notification_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">0.0dip</item>
        <item name="android:paddingRight">0.0dip</item>
        <item name="android:shadowRadius">0.0</item>
    </style>
    <style name="WorkspaceIconTitle.notification.dark" parent="@style/WorkspaceIconTitle.notification">
        <item name="android:textColor">@color/icon_title_text_dark</item>
    </style>
    <style name="SearchButton" parent="@*android:style/Widget.Button.Small">
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:paddingTop">7.0dip</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:paddingBottom">9.0dip</item>
    </style>
    <style name="HotseatButton">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">1.0dip</item>
        <item name="android:layout_weight">0.0</item>
    </style>
    <style name="UninstallDialog">
        <item name="android:textStyle">bold</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
    <style name="UninstallDialog.Title" parent="@style/UninstallDialog">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/uninstall_dialog_title_text</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">@color/uninstall_dialog_title_text_shadow</item>
    </style>
    <style name="UninstallDialog.Message" parent="@style/UninstallDialog">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@color/uninstall_dialog_message_text</item>
        <item name="android:shadowColor">@color/uninstall_dialog_message_text_shadow</item>
    </style>
    <style name="UninstallDialog.Button" parent="@style/UninstallDialog">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/uninstall_dialog_button_text</item>
        <item name="android:shadowColor">@color/uninstall_dialog_button_text_shadow</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="MarqueeTextView">
        <item name="android:ellipsize">marquee</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:singleLine">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>
    <style name="GadgetPlayerLyricCurrentLineShadowStyle">
        <item name="android:shadowColor">@color/gadget_player_lyric_shadow_color</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">0.0</item>
        <item name="android:shadowRadius">10.0</item>
    </style>
    <style name="MultiSelectActionButton">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">5.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="gadget_weather_text_shadow">
        <item name="android:shadowColor">#50000000</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">4.0</item>
        <item name="android:shadowRadius">8.0</item>
    </style>
    <style name="gadget_weather_text_shadow_1">
        <item name="android:shadowColor">#5e000000</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">3.0</item>
        <item name="android:shadowRadius">5.0</item>
    </style>
    <style name="free_style_apps_btn" parent="@*android:style/TextAppearance.DeviceDefault.Medium">
        <item name="android:textSize">@dimen/free_style_apps_btn_text_size</item>
        <item name="android:textColor">#ff3c3c3c</item>
        <item name="android:gravity">bottom|center</item>
        <item name="android:clickable">true</item>
        <item name="android:shadowColor">#cdffffff</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">1.0</item>
        <item name="android:layerType">software</item>
    </style>
    <style name="loading_dialog">
        <item name="android:windowBackground">@drawable/loading_dialog_bg</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowEnterAnimation">@*android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@*android:anim/fade_out</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:windowShowWallpaper">false</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="apply_dialog">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_marginTop">3.0dip</item>
    </style>
</resources>
